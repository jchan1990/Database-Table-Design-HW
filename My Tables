CREATE TABLE Account(
ID INTEGER PRIMARY KEY NOT NULL,
FOREIGN KEY (AccountUserSS) REFERENCES User(SS),
BALANCE INT NOT NULL,
TYPE TEXT NOT NULL,
FOREIGN KEY (AccountTransactionID) REFERENCES Transaction(TRANSACTIONID)
);
CREATE TABLE User(
SS INTEGER PRIMARY KEY NOT NULL,
NAME TEXT NOT NULL,
FOREIGN KEY (UserAccountID) REFERENCES Account(ID),
);
CREATE TABLE Transaction(
TRANSACTIONID INT PRIMARY KEY NOT NULL,
FOREIGN KEY (TransactionAccountID) REFERENCES Account(ID),
AMOUNT INT NOT NULL,
DESCRIPTION TEXT
);
